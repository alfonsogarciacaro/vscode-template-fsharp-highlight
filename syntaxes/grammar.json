{
	"scopeName": "template.fsharp.injection",
	"injectionSelector": "L:source.fsharp - (comment, string.quoted.double.fsharp, string.quoted.triple.fsharp)",
	"patterns": [
		{
			"begin": "[ ._](html|svg) *(?:(?=\\$)|$)",
			"end": "\"{3}",
			"beginCaptures": {
				"1": {
					"name": "variable.fsharp"
				}
			},
			"patterns": [
				{
					"include": "#html_template"
				},
				{
					"include": "source.fsharp"
				}
			]
		},
		{
			"begin": "[ ._](sql) *(?:(?=\\$)|$)",
			"end": "\"{3}",
			"beginCaptures": {
				"1": {
					"name": "variable.fsharp"
				}
			},
			"patterns": [
				{
					"include": "#sql_template"
				},
				{
					"include": "source.fsharp"
				}
			]
		},
		{
			"begin": "[ ._](css) *(?:(?=\\$)|$)",
			"end": "\"{3}",
			"beginCaptures": {
				"1": {
					"name": "variable.fsharp"
				}
			},
			"patterns": [
				{
					"include": "#css_template"
				},
				{
					"include": "source.fsharp"
				}
			]
		}

	],
	"repository": {
		"html_template": {
			"contentName": "meta.embedded.template.html",
			"begin": "\\$\"{3}",
			"end": "(?=\"{3})",
			"patterns": [
				{
					"include": "text.html.derivative"
				}
			]
		},
		"sql_template": {
			"contentName": "meta.embedded.template.sql",
			"begin": "\\$\"{3}",
			"end": "(?=\"{3})",
			"patterns": [
				{
					"include": "source.sql"
				}
			]
		},
		"css_template": {
			"contentName": "meta.embedded.template.css",
			"begin": "\\$\"{3}",
			"end": "(?=\"{3})",
			"patterns": [
				{
					"include": "source.css"
				}
			]
		}
	}
}