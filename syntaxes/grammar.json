{
	"scopeName": "template.fsharp.injection",
	"injectionSelector": "L:source.fsharp - comment",
	"patterns": [
		{
			"begin": "[ ._](html|svg) *(?:(?=\\$)|$)",
			"end": "(?:^|[^$])\"{3}",
			"beginCaptures": {
				"1": {
					"name": "variable.fsharp"
				}
			},
			"patterns": [
				{
					"include": "#html_template"
				},
				{
					"include": "source.fsharp"
				}
			]
		},
		{
			"begin": "[ ._](sql) *(?:(?=\\$)|$)",
			"end": "(?:^|[^$])\"{3}",
			"beginCaptures": {
				"1": {
					"name": "variable.fsharp"
				}
			},
			"patterns": [
				{
					"include": "#sql_template"
				},
				{
					"include": "source.fsharp"
				}
			]
		}
	],
	"repository": {
		"html_template": {
			"contentName": "template.fsharp.html",
			"begin": "\\$\"{3}",
			"end": "(?=(?:^|[^$])\"{3})",
			"patterns": [
				{
					"include": "text.html.derivative"
				}
			]
		},
		"sql_template": {
			"contentName": "template.fsharp.html",
			"begin": "\\$\"{3}",
			"end": "(?=(?:^|[^$])\"{3})",
			"patterns": [
				{
					"include": "source.sql"
				}
			]
		}
	}
}